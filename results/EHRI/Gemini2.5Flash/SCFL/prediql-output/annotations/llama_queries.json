  {
    "query": {
      "query": "\nquery annotations {\n  annotations {\n    items {\n      id\n      type\n      body\n    }\n  }\n}\n",
      "vulnerability_type": "basic_call"
    },
    "response_status": 200,
    "request_time_seconds": 0.662,
